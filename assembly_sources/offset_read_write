#write 0xFF gpio-write-a;
@goto(loop-2-start);
@anchor(loop-start);
write 0x01 stack-push;
write 0x02 stack-push;
write 0x04 stack-push;
write 0x08 stack-push;
#write 0x00 set-stack-offset; write 0x01 offset-write;
#write 0x01 set-stack-offset; write 0x02 offset-write;
#write 0x02 set-stack-offset; write 0x04 offset-write;
#write 0x03 set-stack-offset; write 0x08 offset-write;
write 0x00 set-stack-offset; move offset-read gpio-write-a;
write 0x01 set-stack-offset; move offset-read gpio-write-a;
write 0x02 set-stack-offset; move offset-read gpio-write-a;
write 0x03 set-stack-offset; move offset-read gpio-write-a;
move stack-pop gpio-write-a;
move stack-pop gpio-write-a;
move stack-pop gpio-write-a;
move stack-pop gpio-write-a;
@goto(loop-start);

@anchor(loop-2-start);
write 0x0A stack-push;
write 0x43 stack-push;
write 0x42 stack-push;
write 0x41 stack-push;
#write 0x00 set-stack-offset; write 0x01 offset-write;
#write 0x01 set-stack-offset; write 0x02 offset-write;
#write 0x02 set-stack-offset; write 0x04 offset-write;
#write 0x03 set-stack-offset; write 0x08 offset-write;
write 0x00 set-stack-offset; move offset-read gpio-write-a;
write 0x01 set-stack-offset; move offset-read gpio-write-a;
write 0x02 set-stack-offset; move offset-read gpio-write-a;
write 0x03 set-stack-offset; move offset-read gpio-write-a;
move stack-pop gpio-write-a;
move stack-pop gpio-write-a;
move stack-pop gpio-write-a;
move stack-pop gpio-write-a;
halt;